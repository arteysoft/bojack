<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>alumnos</servlet-name>
    <servlet-class>edu.it.controllers.AlumnosController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>inventarAlumno</servlet-name>
    <servlet-class>edu.it.controllers.InventarAlumnoController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>frontController</servlet-name>
    <servlet-class>edu.it.controllers.FrontController</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
    <servlet-name>alumnos</servlet-name>
    <url-pattern>/alumno</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>alumnos</servlet-name>
    <url-pattern>/alumno/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>frontController</servlet-name>
    <url-pattern>/api/*</url-pattern>
  </servlet-mapping>
  
  <!--  -->
  
  <servlet-mapping>
  	<servlet-name>inventarAlumno</servlet-name>
    <url-pattern>/alumno/inventar</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>inventarAlumno</servlet-name>
    <url-pattern>/alumno/inventar/*</url-pattern>
  </servlet-mapping>
  
</web-app>
